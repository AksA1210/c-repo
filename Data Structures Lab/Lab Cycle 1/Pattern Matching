

//Pattern Matching
#include<iostream>
using namespace std;
void matching(int P[],int R,int T[],int S,int INDEX)
{
    int K=1;
    int MAX=S-R+1;
    while(K<=MAX)
    {
        for(int i=0;i<R;i++)
        {
            if (P[L]!=T[K+L-1])
            {
                return(INDEX);
            }
            INDEX=K;
        }
    }
}
